{
  "name": "fiuba-laboral-front-end",
  "version": "0.1.0",
  "private": true,
  "contributors": [
    "Sebastian Blanco <sebastian.e.blanco@gmail.com>",
    "Dylan Alvarez <dylanalvarez1995@gmail.com>",
    "Manuel Llaur√≥ <llauromanuel@gmail.com>"
  ],
  "dependencies": {
    "@apollo/react-hooks": "3.1.3",
    "@material-ui/core": "4.9.0",
    "@material-ui/icons": "4.5.1",
    "@material-ui/lab": "^4.0.0-alpha.46",
    "@testing-library/dom": "6.11.0",
    "@testing-library/jest-dom": "4.2.4",
    "@testing-library/react": "9.3.2",
    "@testing-library/user-event": "7.1.2",
    "@types/classnames": "2.2.10",
    "@types/humanize-duration": "3.18.0",
    "@types/jest": "24.0.0",
    "@types/lodash": "4.14.149",
    "@types/node": "12.0.0",
    "@types/node-sass": "4.11.0",
    "@types/react": "16.9.34",
    "@types/react-dom": "16.9.6",
    "@types/react-router-dom": "5.1.4",
    "@types/shortid": "0.0.29",
    "apollo-boost": "0.4.7",
    "apollo-client": "2.6.8",
    "apollo-utilities": "1.3.3",
    "classnames": "2.2.6",
    "eslint": "6.8.0",
    "formik": "2.1.4",
    "formik-material-ui": "2.0.0-beta.1",
    "git": "0.1.5",
    "graphql": "14.5.8",
    "graphql-tag": "2.10.1",
    "graphql.macro": "1.4.2",
    "husky": "3.1.0",
    "link-module-alias": "1.2.0",
    "moment": "2.24.0",
    "node-sass": "4.13.0",
    "normalize.css": "8.0.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-router-dom": "5.1.2",
    "react-scripts": "3.4.1",
    "shortid": "2.2.15",
    "tslint": "5.20.1",
    "typescript": "~3.7.2",
    "validations-fiuba-laboral-v2": "1.0.14"
  },
  "resolutions": {
    "@types/react": "16.9.34"
  },
  "scripts": {
    "start": "NODE_ENV=development yarn run postinstall && react-scripts start",
    "build": "react-scripts build",
    "test": "CI=true react-scripts test",
    "eject": "react-scripts eject",
    "lint": "yarn lint:src:ts && yarn lint:src:tsx && yarn lint:test:ts && yarn lint:test:tsx && yarn lint:scss",
    "lint:scss": "yarn stylelint '**/*.scss'",
    "lint:src:ts": "tslint -c tslint.json 'src/**/*.ts'",
    "lint:src:tsx": "tslint -c tslint.json 'src/**/*.tsx'",
    "lint:test:ts": "tslint -c tslint.json 'test/**/*.ts'",
    "lint:test:tsx": "tslint -c tslint.json 'test/**/*.tsx'",
    "preinstall": "which link-module-alias && link-module-alias clean || echo",
    "postinstall": "link-module-alias",
    "stash": "git stash -u --keep-index",
    "unstash": "git stash pop",
    "check-unstaged": "bash scripts/check_unstaged.sh"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn check-unstaged && yarn lint && yarn test"
    }
  },
  "devDependencies": {
    "stylelint": "13.3.3",
    "stylelint-config-sass-guidelines": "7.0.0",
    "stylelint-config-standard": "20.0.0",
    "tslint-config-fiuba-laboral-v2": "1.0.9",
    "tslint-react-hooks": "2.2.1"
  },
  "_moduleAliases": {
    "$root": ".",
    "$package": "package.json",
    "$components": "src/components",
    "$interfaces": "src/interfaces",
    "$pages": "src/pages",
    "$queries": "src/graphql/queries",
    "$mutations": "src/graphql/mutations",
    "$styles": "src/styles",
    "$test": "src/test",
    "$src": "src",
    "$models": "src/models",
    "$config": "src/config",
    "$hooks": "src/models/hooks"
  }
}
